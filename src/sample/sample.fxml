    <children>
      <Button fx:id="hsv" onAction="#setHsv" mnemonicParsing="false" text="HSV" />
        <Button fx:id="button" mnemonicParsing="false" onAction="#onClick" text="Натисніть кнопку" />
    </children>
    <opaqueInsets>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </opaqueInsets>
</VBox>
